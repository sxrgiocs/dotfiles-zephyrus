/* Confirm Dialog */

@import "themes/NightParrot.rasi"

* {
    background-color:       @bg;
    text-color:             @gray;
	font:				 	"Google Sans Medium 12";
}

window {
    width:      			225px;
    padding:    			25px;
    border:					0px;
    border-radius:          4px;
    location:               center;
    y-offset:               -20px;
}

entry {
    expand: 				true;
    text-color:				@gray;
}
